{
  "expo": {
    "name": "Baddil",
    "slug": "Baddil",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/images/splash.png",
        "backgroundColor": "#E60000",
        "resizeMode": "contain",
        "dark": {
          "image": "./assets/images/splash-dark.png",
          "backgroundColor": "#140000"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#E60000"
      },
      "splash": {
        "image": "./assets/images/splash.png",
        "backgroundColor": "#E60000",
        "resizeMode": "contain",
        "dark": {
          "image": "./assets/images/splash-dark.png",
          "backgroundColor": "#140000"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#E60000",
          "image": "./assets/images/splash.png",
          "dark": {
            "image": "./assets/images/splash-dark.png",
            "backgroundColor": "#140000"
          },
          "imageWidth": 200
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
